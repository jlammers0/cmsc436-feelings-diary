<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.feelings_diary">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/feelings_diary_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/feelings_diary_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.feelings_diary.LoginActivity"/>
        <activity android:name="com.example.feelings_diary.RegistrationActivity"/>
        <activity android:name="com.example.feelings_diary.PatientHomeActivity"/>
        <activity android:name="com.example.feelings_diary.TherapistHomeActivity"/>
        <activity android:name="com.example.feelings_diary.MailInboxActivity"/>
        <activity android:name="com.example.feelings_diary.MailMessageActivity"/>
        <activity android:name="com.example.feelings_diary.MailReplyActivity"/>
    </application>

</manifest>